{
	"AWSEBSDockerrunVersion": 2,
	"containerDefinitions": [
	  {
	   	"name": "client",
	   	"image": "brunovpessoa/multi-client",
	   	"hostname": "client",
	   	"essential": false
	  },
	  {
	  	"name": "server",
	  	"image": "brunovpessoa/multi-server",
	  	"hostname": "api",
	  	"essential": false
	  },
	  {
	  	"name": "worker",
	  	"image": "brunovpessoa/multi-worker",
	  	"hostname": "worker",
	  	"essential": false
	  },
	  {
	  	"name": "nginx",
	  	"image": "brunovpessoa/multi-nginx",
	  	"hostname": "nginx",
	  	"essential": true,
	  	"portMappings": [
	  	  {
	  	  	"hostPort": 80,
	  	  	"containerPort": 80
	  	  }
	  	],
	  	"links": ["client", "server"]

	  }	
	]
}